{
  "name": "slack-export",
  "version": "1.0.0",
  "description": "A utility for exporting Slack data, written in Node.js and typescript ",
  "main": "index.js",
  "scripts": {
    "build": "rimraf ./build && tsc",
    "start:dev": "nodemon",
    "start": "node dist-prod/index.js",
    "lint": "eslint . --ext .ts",
    "prod": "tsc --outDir dist-prod"
  },
  "keywords": [
    "slack",
    "export",
    "node",
    "typescript"
  ],
  "author": "Dave Caslin",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^14.14.41",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@slack/web-api": "^6.1.0",
    "axios": "^0.21.1",
    "chalk": "^4.1.0",
    "conf": "^9.0.2",
    "date-fns": "^2.21.1",
    "dotenv": "^8.2.0",
    "sanitize-filename": "^1.6.3"
  }
}
